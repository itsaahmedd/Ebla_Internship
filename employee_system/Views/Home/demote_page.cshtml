    <script>$(document).ready(function () {


            $.ajax({
                url: '/Home/GetUsers',
                type: 'GET',
                dataType: 'json',
                success: function (response) {
                    console.log(response);

                    for (var i = 0; i < response.length; i++) {
                        var row = "<tr>";
                        row += "<td>" + response[i].id + "</td>";
                        row += "<td>" + response[i].username + "</td>";
                        row += "<td>" + response[i].first_Name + "</td>";
                        row += "<td>" + response[i].last_Name + "</td>";
                        row += "<td>" + response[i].phone_Number + "</td>";
                        row += "<td>" + response[i].department + "</td>";
                        row += "<td>" + (response[i].level === 1 ? "Normal" : (response[i].level === 2 ? "Team Leader" : (response[i].level === 3 ? "HR" : ""))) + "</td>";

                        row += "</tr>";
                        $('#myTable tbody').append(row);
                    }

                    $('#myTable').DataTable({
                        paging: true,
                        searching: true,
                        ordering: true,
                        responsive: true
                    });

                    // Add a click event to open the modal when clicking on a row
                    $('#myTable tbody').on('click', 'tr', function () {
                        var rowData = $('#myTable').DataTable().row(this).data();
                        displayRequestModal(rowData);
                    });

                    function displayRequestModal(data) {
                        var modalContent = '<div class="modal-info">';
                        modalContent += '<div class="header-row">';
                        modalContent += '<p><strong>Username:</strong> ' + data[1] + '</p>';
                        modalContent += '<p><strong>ID:</strong> ' + data[0] + '</p>';
                        modalContent += '</div>';
                        modalContent += '<p><strong>Fiirst Name:</strong> ' + data[2] + '</p>';
                        modalContent += '<p><strong>Last Name:</strong> ' + data[3] + '</p>';
                        modalContent += '<p><strong>Phone Number:</strong> ' + data[4] + '</p>';

                        modalContent += '<p><strong>Department:</strong> ' + data[5] + '</p>';
                        modalContent += '</div>';

                        $('#modal-content').html(modalContent);
                        $('#requestModal').modal('show');

                        $("#acceptButton").click(function () {
                            $.ajax({
                                url: "/Home/demote",
                                type: "POST",
                                data: {
                                    email: data[1],
                                },
                                success: function (response) {
                                    console.log(response)
                                    if (response === "User doesn't exist") {
                                        Swal.fire({
                                            title: 'Error!',
                                            text: 'User is not found',
                                            icon: 'warning',
                                            confirmButtonText: 'Continue'
                                        });
                                    } else if (response == "Request accepted successfully") {
                                        Swal.fire({
                                            title: 'Success!',
                                            text: 'User has been demoted',
                                            icon: 'success',
                                            confirmButtonText: 'Continue'
                                        });
                                    }
                                },
                                error: function (x, y, z) {
                                    console.log(x, y, z);
                                    Swal.fire({
                                        title: 'Error!',
                                        text: 'Something went wrong with your request',
                                        icon: 'error',
                                        confirmButtonText: 'Continue'
                                    });
                                }
                            });
                        });
                    }
                },
                error: function (xhr, status, error) {
                    console.error('Ajax request error:', error);
                }
            });

        });</script>



